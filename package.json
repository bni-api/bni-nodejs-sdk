{
  "name": "bni-nodejs",
  "version": "0.1.21",
  "description": "Official BNI API SDK for Node.js",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test": "mocha --timeout 10000",
    "lint": "eslint . --fix",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./afterbuild.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bni-api/bni-nodejs-sdk.git"
  },
  "author": "BNI API",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bni-api/bni-nodejs-sdk/issues"
  },
  "homepage": "https://github.com/bni-api/bni-nodejs-sdk#readme",
  "dependencies": {
    "axios": "^1.1.3",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "eslint": "^8.27.0",
    "mocha": "^10.1.0",
    "sinon": "^15.2.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
